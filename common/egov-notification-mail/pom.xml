      <!-- Bouncy Castle: provides general-purpose crypto (OpenSSL equivalent in Java) -->
<dependency>
<groupId>org.bouncycastle</groupId>
<artifactId>bcprov-jdk15on</artifactId>
<version>1.76</version>
</dependency>
 
<!-- Bouncy Castle PKIX/CMS/PKCS#11 support if needed -->
